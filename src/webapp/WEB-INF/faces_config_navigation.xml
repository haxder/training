<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

   <navigation-rule>
      <from-view-id>index.xhtml</from-view-id>
      <navigation-case>
       <!--   <from-action>#{navigatorController.navi_MoviesListView}</from-action>  -->
         <from-outcome>MOVIES</from-outcome>
         <to-view-id>/resource/views/movies.xhtml</to-view-id>
      </navigation-case>
      <navigation-case>
       <!--   <from-action>#{navigationController.processPage2}</from-action>  -->
         <from-outcome>MOVIEDETAIL</from-outcome>
         <to-view-id>/resource/views/movieDetail.xhtml</to-view-id>
      </navigation-case>
   </navigation-rule>
</faces-config>
